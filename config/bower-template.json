{
  "name": "<%= pkg.name %>",
  "homepage": "<%= pkg.homepage %>",
  "version": "<%= pkg.version %>",
  "authors": [
      "<%= pkg.author %>", 
      "<%= pkg.contributors %>"
  ],
  "description": "<%= pkg.description %>",
  "main": "dist/LeafletPlayback.js",
